# File managed by puppet

user <%= @process_user %>;
worker_processes 512;

error_log <%= @log_dir%>/error.log;
pid <%= @pid_file %>

events{
  worker_connection 1024;
}
http{
  server_token off;
  include <%= @config_dir %>/mime.type;
  default_type application/octet-stream;
  access_log <%= @log_dir%>/access.log;
  sendfile on;
  #tcp_nonpass on;
  tcp_nodelay on;
}

include <%= @confd%>/*.conf;
<% if @vdir_enble %>
  include <%= @vdir_enable %>/ *;
<% end %>


